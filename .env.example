 ## Gemini API Configuration
# Put your Gemini API key here (get from Google AI Studio)
GEMINI_API_KEY= 
# Model name for Gemini (default: models/gemini-flash-latest)
GEMINI_MODEL=models/gemini-flash-latest
 
## Hugging Face Configuration
# Put your Hugging Face token here (get from https://huggingface.co/settings/tokens)
HUGGINGFACE_TOKEN= 

## Supabase Configuration
# Supabase project URL (get from Supabase dashboard)
SUPABASE_URL= 
# Supabase anon/public key (get from Supabase dashboard)
SUPABASE_KEY= 
# Supabase service role key (optional, for admin access)
SUPABASE_SERVICE_ROLE_KEY= 

## Database Configuration
# Database connection string (e.g. postgresql://user:pass@host:port/dbname)
DATABASE_URL= 


## Qdrant Configuration
# Qdrant API key (if required)
QDRANT_API_KEY= 
# Qdrant server URL (e.g. http://localhost:6333)
QDRANT_URL= 

## Sentence Transformers Model (local embeddings, no API required)
# Model name for local embeddings
SENTENCE_TRANSFORMERS_MODEL=sentence-transformers/all-mpnet-base-v2

## General App Settings
APP_ENV=development
LOG_LEVEL=INFO
DEBUG=true

## Model Configuration (StatPearls + Gemini)
# Embedding dimension for models
EMBEDDING_DIMENSION=768
# Maximum chunk size for text splitting
MAX_CHUNK_SIZE=400
# Overlap size between chunks
CHUNK_OVERLAP=80

## StatPearls Ingestion Configuration (TEST MODE)
# Maximum number of StatPearls chunks to ingest (test mode)
MAX_STATPEARLS_CHUNKS=2000
# Minimum content length for ingestion
MIN_CONTENT_LENGTH=300

## Retrieval Configuration
# Number of top results to retrieve
TOP_K_RETRIEVAL=10
# Similarity threshold for retrieval
SIMILARITY_THRESHOLD=0.7

## API Configuration
# Host and port for running the API server
API_HOST=0.0.0.0
API_PORT=8000
# Allowed CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173